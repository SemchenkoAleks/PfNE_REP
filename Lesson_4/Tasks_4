In [5]: mac = "AAAA:BBBB:CCCC"                                                                          

In [6]: mac.replace(':','.')                                                                            
Out[6]: 'AAAA.BBBB.CCCC'


3

In [35]: config = "switchport trunk allowed vlan 1,3,10,20,30,100"                                      

In [36]: config2 = config.split()                                                                       

In [37]: config2                                                                                        
Out[37]: ['switchport', 'trunk', 'allowed', 'vlan', '1,3,10,20,30,100']

In [38]: config3 = config2[-1]                                                                          

In [39]: config3                                                                                        
Out[39]: '1,3,10,20,30,100'

In [40]: config3.split(',')                                                                             
Out[40]: ['1', '3', '10', '20', '30', '100']

In [41]: config3                                                                                        
Out[41]: '1,3,10,20,30,100'

In [42]: config4 = config3.split(',')                                                                   

In [43]: config4                                                                                        
Out[43]: ['1', '3', '10', '20', '30', '100']

